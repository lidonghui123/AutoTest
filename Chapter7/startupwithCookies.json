    "description": "这是一个带有cookies的mock请求",
    "request": {
      "uri": "/getCookies",
      "method": "get"
    },
    "response": {
      "cookies": {
        "login": "true",
        "status": "10000"
      },
      "text": "恭喜你获得cookies信息"
    }
