      "headers": {
        "Content-Type": "text/html:charset=gbk"
      }
    }
  },
  {
    "description": "这是待cookies信息的post请求",
    "request": {
      "uri": "/post/with/cookies",
      "method": "post",
      "cookies": {
        "login": "true"
      },
      "json": {
        "name": "huhansan",
        "age": "18"
      }
    },
    "response": {
      "status": 200,
      "json": {
        "huuhansan": "success",
        "status": "1",
        "text": "可以啦"
      }
